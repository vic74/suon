anAll+Q1.FieldByName('planed').Value;&#13;&#10;    readyAll:=readyAll+Q1.FieldByName('prov').Value;                                                          &#13;&#10;    Q1.Next;                     &#13;&#10;  end;}  &#13;&#10;        &#13;&#10;  //ShowMessage(IntToStr(readyAll));&#13;&#10;  Q2.Sql.Text:=&#13;&#10;       'SELECT count(*) planP from showhouseplan'+    &#13;&#10;       ' where showhouse=:sh and (d1 &#60;=:d1 and d2&#62;=:d1)';    &#13;&#10;  &#13;&#10; { 'select s.SRVDOG,1 planed, iif(sp.showdate is null, 0,1) prov'+    &#13;&#10;  ' from SHOWHOUSE s'+    &#13;&#10;  ' left outer join showhouseplan sp on s.code=sp.showhouse'+&#13;&#10;  ' where s.srvdog=:dog and sp.d1 &#62;= :d1 and sp.d1&#60;=:d2';}  &#13;&#10;  Q2.ParamByName('sh').Value:=Report.variables['varAct'];&#13;&#10;  Q2.ParamByName('d1').Value:=dt1;&#13;&#10;  //Q2.ParamByName('d2').Value:=dt2;&#13;&#10;  Q2.Open;&#13;&#10;  planP:=Q2.FieldByName('planP').Value;&#13;&#10;  Q2.Close;&#13;&#10;    &#13;&#10;  Q2.Sql.Text:=&#13;&#10;       'SELECT count(*) readyP from showhouseplan'+    &#13;&#10;       ' where showhouse=:sh and showdate between :d1 and :d2';        &#13;&#10;  &#13;&#10;  Q2.ParamByName('sh').Value:=Report.variables['varAct'];&#13;&#10;  Q2.ParamByName('d1').Value:=dt1;&#13;&#10;  Q2.ParamByName('d2').Value:=dt2;&#13;&#10;  Q2.Open;  &#13;&#10;  readyP:=Q2.FieldByName('readyP').Value;      &#13;&#10;  {while not Q2.Eof do&#13;&#10;  begin&#13;&#10;    planP:=planP+Q2.FieldByName('planed').Value;&#13;&#10;    readyP:=readyP+Q2.FieldByName('prov').Value;                                                          &#13;&#10;    Q2.Next;                     &#13;&#10;  end;}        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo21OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if planP&#62;0 then&#13;&#10;  Memo21.Text:='[(&#60;readyP&#62;*100/&#60;planP&#62;)]%' else&#13;&#10;  Memo21.Text:='0';                                  &#13;&#10;end;&#13;&#10;&#13;&#10;